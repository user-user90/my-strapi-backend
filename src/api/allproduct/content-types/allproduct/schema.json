{
  "kind": "collectionType",
  "collectionName": "allproducts",
  "info": {
    "singularName": "allproduct",
    "pluralName": "allproducts",
    "displayName": "allProduct"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "blocks"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "BAGS",
        "SHIRTS",
        "DRINKWARE",
        "FOOTWARE",
        "HEADWEAR"
      ]
    },
    "sliderproducts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sliderproduct.sliderproduct",
      "mappedBy": "allproduct"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
