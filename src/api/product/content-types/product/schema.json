{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "heroSection"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "blocks"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "shirts"
      ]
    },
    "navbars": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::navbar.navbar",
      "mappedBy": "product"
    }
  }
}
